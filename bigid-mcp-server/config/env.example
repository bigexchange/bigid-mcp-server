# BigID Configuration
BIGID_DOMAIN=sandbox.bigid.tools
BIGID_AUTH_TYPE=session
BIGID_USERNAME=your_username
BIGID_PASSWORD=your_password

# Alternative: User Token Authentication
# BIGID_AUTH_TYPE=user_token
# BIGID_USER_TOKEN=your_user_token

# BigID Advanced Settings
BIGID_TIMEOUT=30000
BIGID_RETRY_ATTEMPTS=3

# MCP Server Configuration
MCP_PORT=3000
MCP_HOST=localhost
MCP_LOG_LEVEL=info
MCP_CONFIG_FILE=config.json 