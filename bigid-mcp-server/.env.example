# BigID MCP Server Environment Configuration
# Copy this file to .env and update with your values

# Authentication Configuration
BIGID_USER_TOKEN=your-user-token-here
BIGID_DOMAIN=your-bigid-domain.com
BIGID_AUTH_TYPE=user_token

# Optional: Session Authentication (alternative to user token)
# BIGID_USERNAME=your-username
# BIGID_PASSWORD=your-password

# API Configuration
BIGID_TIMEOUT=30000
BIGID_RETRY_ATTEMPTS=3

# Environment Configuration
NODE_ENV=production
BIGID_MCP_LOG_LEVEL=info
